<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.history.entity">
	<class name="History" table="history" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		
				<property name="historyType" column="historyType" type="string"></property>
				<property name="whoId" column="whoId" type="int"></property>
				<property name="whoclassName" column="whoclassName" type="string"></property>
				<property name="whoName" column="whoName" type="string"></property>
				
				<property name="title" column="title" type="string"></property>
				<property name="relDate" column="relDate" type="java.sql.Timestamp"></property>
				
				<property name="ip" column="ip" type="string"></property>
				<property name="extraData" column="extraData" type="string"></property>
				<property name="addressStr" column="addressStr" type="string"></property>
				
	</class>
</hibernate-mapping>