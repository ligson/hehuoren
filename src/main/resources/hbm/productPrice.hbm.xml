<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.product.entity">
	<class name="ProductPrice" table="product_productPrice">
        <id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="productId" column="productId" type="int"></property>	
		<property name="productName" column="productName" type="string"></property>		
		<property name="productImg" column="productImg" type="string"></property>	
		<property name="xilieName" column="xilieName" type="string"></property>		
