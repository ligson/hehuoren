		<property name="yesproductPrice" column="yesproductPrice" type="int"></property>
		
		
		
		
		<property name="productCount" column="productCount" type="int"></property>
		<property name="saleCount" column="saleCount" type="int"></property>
		
		
		
		
		
		<property name="pickupAddressIds" column="pickupAddressIds" type="string"></property>
		
		
		<property name="keyName1" column="keyName1" type="string"></property>
		<property name="keyName2" column="keyName2" type="string"></property>
		<property name="keyName3" column="keyName3" type="string"></property>
		<property name="keyName4" column="keyName4" type="string"></property>
		<property name="keyName5" column="keyName5" type="string"></property>
		
    </class>
    
</hibernate-mapping>