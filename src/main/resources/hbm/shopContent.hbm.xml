<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.shop.entity">
	<class name="ShopContent" table="shop_shopContent" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		
				<property name="otherContent" column="otherContent" type="string"></property>
				<property name="content" column="content" type="string"></property>
				<property name="shopId" column="shopId" type="int"></property>
	</class>
</hibernate-mapping>