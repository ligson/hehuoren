<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.user.entity">
	<class name="Shoucang" table="user_shoucang" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		
				<property name="userId" column="userId" type="int"></property>
				<property name="scEntity" column="scEntity" type="string"></property>
				<property name="scEntityId" column="scEntityId" type="int"></property>
				<property name="relDate" column="relDate" type="java.sql.Timestamp"></property>
	
	</class>
</hibernate-mapping>