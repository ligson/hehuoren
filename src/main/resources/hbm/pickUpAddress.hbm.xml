<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="hhr.order.entity">
	<class name="PickUpAddress" table="order_pickUpAddress">
        <id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="addressStr" column="addressStr" type="string"></property>	
		<property name="telPhone" column="telPhone" type="string"></property>	
		<property name="name" column="name" type="string"></property>	
		<property name="tpName" column="tpName" type="string"></property>	
		
		<property name="markedNum" column="markedNum" type="string"></property>
			
		<property name="addressId" column="addressId" type="int"></property>	
		<property name="addressTreeIds" column="addressTreeIds" type="string"></property>	
    </class>
    
</hibernate-mapping>