<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.shop.entity">
	<class name="ShopLevel" table="shop_shopLevel" lazy="true" >
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="presenceInPrice" column="presenceInPrice" type="float"></property>
		<property name="specialPrice" column="specialPrice" type="float"></property>
		<property name="specialReturnPlan" column="specialReturnPlan" type="string"></property>
		<property name="webSpecialReturnPlan" column="webSpecialReturnPlan" type="string"></property>
		<property name="shopMainBigProductTypeCount" column="shopMainBigProductTypeCount" type="int"></property>
		
				
	</class>
</hibernate-mapping>