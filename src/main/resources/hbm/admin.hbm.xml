<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.admin.entity">
	<class name="Admin" table="admin" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
				<property name="name" column="name" type="string"></property>
				<property name="password" column="password" type="string"></property>
				<property name="superOrCom" column="superOrCom" type="int"></property>
				<property name="status" column="status" type="int"></property>


			<property name="telPhone" column="telPhone" type="string"></property>
			<property name="addressId" column="addressId" type="int"></property>
			<property name="addressTreeIds" column="addressTreeIds" type="string"></property>
			<property name="cityName" column="cityName" type="string"></property>
			<property name="isCanAccount" column="isCanAccount" type="int"></property>
			
			<property name="wxId" column="wxId" type="string"></property>
	</class>
</hibernate-mapping>