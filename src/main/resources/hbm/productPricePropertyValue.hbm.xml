<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.product.entity">
	<class name="ProductPricePropertyValue" table="product_productPricePropertyValue">
        <id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="productId" column="productId" type="int"></property>	
		<property name="keyPy" column="keyPy" type="string"></property>		
		<property name="keyName" column="keyName" type="string"></property>	
		<property name="keyValues" column="keyValues" type="string"></property>		
		
		
    </class>
    
</hibernate-mapping>