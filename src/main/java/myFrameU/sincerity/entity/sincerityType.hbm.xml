<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.sincerity.entity">
	<class name="SincerityType" table="sincerity_sincerityType" lazy="true" >
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="typeName" column="typeName" type="string"></property>
		<property name="typeKey" column="typeKey" type="string"></property>
		<property name="addOrMis" column="addOrMis" type="string"></property>
		<property name="score" column="score" type="float"></property>
	</class>
</hibernate-mapping>