<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.user.entity">
	<class name="MyAddress" table="user_myAddress" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		
				<property name="userId" column="userId" type="int"></property>
				<property name="addressId" column="addressId" type="int"></property>
				<property name="addressTreeIds" column="addressTreeIds" type="string"></property>
				
				<property name="name" column="name" type="string"></property>
				<property name="jutiAddress" column="jutiAddress" type="string"></property>
				<property name="zipcode" column="zipcode" type="string"></property>
				<property name="telPhone" column="telPhone" type="string"></property>
				<property name="phone" column="phone" type="string"></property>
				<property name="isDefault" column="isDefault" type="int"></property>
				
	<property name="qq" column="qq" type="string"></property>
	<property name="email" column="email" type="string"></property>
	<property name="msn" column="msn" type="string"></property>
	<property name="ww" column="ww" type="string"></property>
	
	
	
	
	
	
	
	
		<property name="coorX" column="coorX" type="double"></property>
		<property name="coorY" column="coorY" type="double"></property>
	
	
	
	
	
	
	
	
	</class>
</hibernate-mapping>