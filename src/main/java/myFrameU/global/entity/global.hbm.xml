<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="myFrameU.global.entity">
	<class name="Global" table="webGlobal" lazy="true">
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="namePy" column="namePy" type="string"></property>
		<property name="myValue" column="myValue" type="string"></property>
		<property name="bz" column="bz" type="string"></property>
		<property name="xm" column="xm" type="string"></property>
	</class>
</hibernate-mapping>