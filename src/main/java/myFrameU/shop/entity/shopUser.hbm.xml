<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="myFrameU.shop.entity">
	<class name="ShopUser" table="shop_shopUser" lazy="true" >
		<id name="id" column="id">
			<generator class="identity">
			</generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="password" column="password" type="string"></property>
		<property name="lianxiren" column="lianxiren" type="string"></property>
		<property name="lianxirenPhone" column="lianxirenPhone" type="string"></property>
		
		<property name="shopId" column="shopId" type="int"></property>
		<property name="addressTreeIds" column="addressTreeIds" type="string"></property>
		<property name="addressId" column="addressId" type="int"></property>
		
		
		<property name="shopName" column="shopName" type="string"></property>
				<property name="wxId" column="wxId" type="string"></property>
	</class>
</hibernate-mapping>